<p id="notice"><%= notice %></p>

<h1>Jobrecs</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Email address</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @jobrecs.each do |jobrec| %>
      <tr>
        <td><%= jobrec.first_name %></td>
        <td><%= jobrec.last_name %></td>
        <td><%= jobrec.email_address %></td>
        <td><%= link_to 'Show', jobrec %></td>
        <td><%= link_to 'Edit', edit_jobrec_path(jobrec) %></td>
        <td><%= link_to 'Destroy', jobrec, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Jobrec', new_jobrec_path %>
